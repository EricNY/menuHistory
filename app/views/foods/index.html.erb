<% @decade["dishes"][0..9].each do |dish|  %>
  <div style="inline-block" class="boxed">
    <h1 data-name=<%=dish["name"]%>><%= dish["name"] %></h1>
  <div style="inline-block;">
    <h1><%= dish["name"] %></h1>
    <h4>On <%= dish["menus_appeared"]%> menus</h4>
    <h4>First appeared in <%= dish["first_appeared"] %></h4>
    <h4>Last appeared in <%= dish["last_appeared"] %></h4>
    <a href=<%= locu_path(dish["name"]) %>><%= dish["name"] %></a>
  </div>
<% end %>
